# CONFIG_REGULATOR_QPNP_LCDB is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_TCM is not set
# CONFIG_TOUCHSCREEN_SYNAPTICS_DSX is not set
# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_FTS_DIRECTORY is not set
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_UFSTW is not set
CONFIG_CRYPTO_CHACHA20POLY1305=y
CONFIG_CAM_SENSOR_PROBE_RETRY=y
# CONFIG_SND_SOC_AW87XXX is not set
CONFIG_AW882XX_STEREO_SMARTPA=y
CONFIG_AW882XX_PARAMS_FROM_BIN=y
CONFIG_AW882XX_MAPSIZE_16K=y
# CONFIG_UFSFEATURE is not set
# CONFIG_UFSHPB is not set
# CONFIG_UFSTW is not set
# CONFIG_SCSI_SKHPB is not set

CONFIG_USB_CONFIGFS_F_UVC=y
CONFIG_USB_DWC3_RT_AFFINITY=y
