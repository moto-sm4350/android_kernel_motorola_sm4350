CONFIG_INPUT_TOUCHSCREEN_FOCALTECH_0FLASH_V2_MMI_PANEL_NOTIFICATIONS=y
CONFIG_TOUCHSCREEN_NT36xxx_HOSTDL_SPI_MMI_V1=m
# CONFIG_INPUT_NOVA_0FLASH_MMI_ENABLE_DOUBLE_TAP is not set
CONFIG_INPUT_NOVA_0FLASH_MMI_NOTIFY_TOUCH_STATE=y
CONFIG_INPUT_NOVA_PANEL_NOTIFICATIONS=y
CONFIG_INPUT_NOVA_DRM_PANEL=y
# CONFIG_INPUT_NOVA_CHIP_VER_1 is not set
CONFIG_NOVA_ESD_ENABLE=y
