CONFIG_WATCHDOGTEST=m
# CONFIG_WATCHDOG_CPU_CTX is not set
CONFIG_QPNP_ADAPTIVE_CHARGE=m
CONFIG_SGM4154X_CHARGER=m
CONFIG_TCPC_CLASS=m
CONFIG_RT_PD_MANAGER=m
CONFIG_BQ2589X_CHARGER=m
